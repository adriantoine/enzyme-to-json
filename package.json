    "email": "adrien@tray.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trayio/enzyme-to-json.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.compact": "^3.0.1",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "lodash.pickby": "^4.6.0"
  },
  "peerDependencies": {
