{
  "version": "3.0.0-beta4",
  "name": "enzyme-to-json",
  "description": "convert enzyme wrapper to a format compatible with Jest snapshot",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d ./",
    "clean": "rm *.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint src tests/core && npm run format-check",
    "format": "prettier src/**/*.js tests/core/*.js tests/core/**/*.js --write --single-quote --trailing-comma all --no-bracket-spacing",
    "format-check": "prettier-check src/**/*.js tests/core/*.js tests/core/**/*.js --write --single-quote --trailing-comma all --no-bracket-spacing",
    "test": "npm run lint && npm run test-core",
    "test-core": "NODE_ENV=test jest --coverage"
  },
  "keywords": [
    "enzyme",
    "jest",
    "snapshot",
    "wrapper",
    "pretty-format",
    "cheerio",
    "convert",
    "testing",
    "unit",
    "test"
  ],
  "author": "Adrien Antoine <adriantoine@gmail.com> (http://adriantoine.com)",
  "homepage": "https://github.com/adriantoine/enzyme-to-json#readme",
  "bugs": {
    "url": "https://github.com/adriantoine/enzyme-to-json/issues",
    "email": "adrien@tray.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriantoine/enzyme-to-json.git"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "enzyme": "^3.0.0-beta.3"
  },
  "devDependencies": {
    "babel-cli": "^7.0.0-beta.0",
    "babel-core": "^7.0.0-beta.0",
    "babel-plugin-syntax-export-extensions": "^7.0.0-beta.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.0",
    "babel-plugin-transform-react-jsx": "^7.0.0-beta.0",
    "babel-preset-env": "^2.0.0-beta.0",
    "codecov": "^2.3.0",
    "enzyme": "^3.0.0-beta.3",
    "enzyme-adapter-react-16": "^1.0.0-beta.2",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "~21.1.0",
    "prettier": "^1.7.0",
    "prettier-check": "^1.0.0",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-test-renderer": "^16.0.0-rc.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests"
    ]
  }
}
