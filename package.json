{
  "name": "enzyme-to-json",
  "description": "convert enzyme wrapper to a format compatible with Jest snapshot",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "prepublish": "npm run build",
    "test": "eslint src test && NODE_ENV=test jest --coverage",
